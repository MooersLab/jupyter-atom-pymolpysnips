      $0
    '''
  'Fetch fofc map from the PDB.':
    'prefix': 'fetchFoFc'
    'body': '''
cmd.do("fetch ${1:3nd4},  ${1:3nd4}_fofc, type=fofc, async=0;")
      $0
    '''
  'Filled rings in nucleic acids.':
    'prefix': 'filledRing'
