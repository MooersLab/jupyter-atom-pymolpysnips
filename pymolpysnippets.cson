cmd.do("hide (name H*);")
cmd.do("hide lines;")
cmd.do("show sticks;")
cmd.do("set stick_radius, ${2:1.2};")
cmd.do("set nb_sphere_radius, ${3:0.35};")
cmd.do("orient;")
      $0
    '''
  'Load PDB nb spheres.':
    'prefix': 'loadPDBnb'
